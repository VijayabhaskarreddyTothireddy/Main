{
	"name": "Variable_employee",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "LS_Azuredatalake",
						"type": "DatasetReference"
					},
					"name": "employee"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_EMP",
						"type": "DatasetReference"
					},
					"name": "EMP",
					"rejectedDataLinkedService": {
						"referenceName": "LS_ADLS",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {First Name} as string,",
				"          {Last Name} as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Gender as string,",
				"          Age as string,",
				"          {Job Title} as string,",
				"          {Years Of Experience} as string,",
				"          Salary as string,",
				"          Department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> employee",
				"employee sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {First Name} as string,",
				"          {Last Name} as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Gender as string,",
				"          Age as string,",
				"          {Job Title} as string,",
				"          {Years Of Experience} as string,",
				"          Salary as string,",
				"          Department as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> EMP"
			]
		}
	}
}