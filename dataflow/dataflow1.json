{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_blob_salaries",
						"type": "DatasetReference"
					},
					"name": "Salaries"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_blob_salaries",
						"type": "DatasetReference"
					},
					"name": "destinatio",
					"rejectedDataLinkedService": {
						"referenceName": "LS_BlobStorage",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Name as string,",
				"          Age as short,",
				"          Job as string,",
				"          Salary as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Salaries",
				"Salaries sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Name as string,",
				"          Age as string,",
				"          Job as string,",
				"          Salary as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> destinatio"
			]
		}
	}
}